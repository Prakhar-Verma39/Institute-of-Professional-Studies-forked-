<% layout('layouts/boilerplate') %>



  <div class="col-8">    

    <h1 class="text-center">All Navbar Items</h1>
    <div class="mb-3 d-grid gap-2 d-md-flex justify-content-md-end">
        <a class="btn btn-danger" href="/navbarItems/new">Add a Navbar Item</a>
    </div>
   
    <% for (let c of navbarItems) { %>
        <div class="card mb-3">
            <div class="row">
                <div class="card-body ps-4">
                    <h5 class="card-title"><%= c.item %></h5>
                    <p class="card-text"><b>LINK : </b><%= c.link %></p>
                    <a class="btn btn-outline-primary" href="/navbarItems/<%=c._id%>">View more...</a>
                </div>    
            </div>
        </div>
    <% }%>
    </div>    

