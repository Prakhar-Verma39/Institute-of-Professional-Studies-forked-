<div class="bg">
  <div class="navbarContainer">
    <nav id="navbar" class="navbar">
      <div class="logo">
        <img src="<%= page.path %>assets/logo.png" alt="logo" />
      </div>
 
      
      <ul class="list">
        
        <% for(let navbarItem of navbarItems){ %>
        <li class="list-item">
          <a class="list-item" aria-current="page" href="<%= navbarItem.link %>"><%= navbarItem.item %></a>
          
          <% if( navbarItem.subItems.length  !== 0) {%>
              <div class="dropdown-content">
              <% for(let i = 0;i < navbarItem.subItems.length; i++){ %>         
                  <a href="<%= navbarItem.subLinks[i] %>"><%= navbarItem.subItems[i] %></a>
              <% } %> 
          </div>
          <% } %> 
        </li>
        <% } %>
      </ul>
      <div class="mobile">
        <svg viewBox="0 0 100 80" width="30" height="30">
          <rect fill="#FFFFFF" width="100" height="10"></rect>
          <rect fill="#FFFFFF" y="30" width="100" height="10"></rect>
          <rect fill="#FFFFFF" y="60" width="100" height="10"></rect>
        </svg>
      </div>
      <ul class="mobile-list">
        
        <% for(let navbarItem of navbarItems){ %>
          <li class="list-item">
            <a class="list-item"  aria-current="page" href="<%= navbarItem.link %>"><%= navbarItem.item %></a>
            
            <% if( navbarItem.subItems.length  !== 0) {%>
                <div class="dropdown-content">
                <% for(let i = 0;i < navbarItem.subItems.length; i++){ %>         
                    <a href="<%= navbarItem.subLinks[i] %>"><%= navbarItem.subItems[i] %></a>
                <% } %> 
            </div>
            <% } %> 
          </li>
          <% } %>
     </ul>
     
    </nav>
  </div>
